<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.hibernate.pojo.Employee" table="employeecid">
		<composite-id>
			<key-property name="employeeId" column="eid" />
			<key-property name="employeeName" column="ename" length="15" />
		</composite-id>
		<property name="salary" column="sal" />
	</class>
</hibernate-mapping>